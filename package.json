{
  "name": "seo-ai",
  "version": "0.4.0",
  "description": "Command-line tool to generate SEO metadata and HTML meta tags using AI models",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "ai",
    "seo",
    "metadata",
    "metatags"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xavimondev/seo-ai.git"
  },
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "tsc --noEmit",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "version:add": "changeset add",
    "version:status": "changeset status --verbose",
    "version:save": "changeset version",
    "release": "changeset publish"
  },
  "author": {
    "name": "Xavi Alfaro",
    "url": "https://x.com/xavimonp"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.7",
    "@types/node": "20.14.10",
    "prettier": "3.3.2",
    "rimraf": "6.0.1",
    "tsup": "8.1.0",
    "type-fest": "4.21.0",
    "typescript": "5.5.3"
  },
  "dependencies": {
    "@ai-sdk/mistral": "0.0.22",
    "@ai-sdk/openai": "0.0.36",
    "@clack/prompts": "0.7.0",
    "ai": "3.2.19",
    "chalk": "5.3.0",
    "commander": "12.1.0",
    "conf": "13.0.1",
    "replicate": "0.31.1",
    "zod": "3.23.8"
  },
  "engines": {
    "node": ">=18"
  }
}